<div class='container px-5 px-sm-0'>
  <div class='row'>
    <table class='table'>
      <thead>
        <tr class="table-outline-secondary">
          <th class="color"><strong>Profile-Image</strong></th>
          <th class="color"><strong>User Name</strong></th>
          <th class="color"><strong>Go to user profile</strong></th>
        </tr>
      </thead>
      <tbody>
        <% users.each do |user| %>
          <tr class="table-dark">
            <td><%= link_to admin_user_path(user) do %>
              <%= image_tag user.get_profile_image, class: "rounded-circle", size: '100x100' %>
              <% end %>
              </td>
            <td class="color"><%= user.name %></td>
            <td><%= link_to 'User_Profile', admin_user_path(user), class: "user_#{user.id}", class: "btn btn-info" %></td>
          </tr>
        <% end %>
         <%= paginate users %>
      </tbody>
    </table>
    </div>
</div>